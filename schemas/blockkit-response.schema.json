{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "text": {
      "type": "string",
      "minLength": 1,
      "maxLength": 3000
    },
    "blocks": {
      "type": "array",
      "minItems": 1,
      "maxItems": 50,
      "items": { "$ref": "#/$defs/block" }
    }
  },
  "required": ["text", "blocks"],
  "additionalProperties": false,
  "$defs": {
    "blockId": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "actionId": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "url": {
      "type": "string",
      "minLength": 1,
      "maxLength": 3000
    },
    "plainTextObject": {
      "type": "object",
      "properties": {
        "type": { "const": "plain_text" },
        "text": { "type": "string", "minLength": 1, "maxLength": 3000 },
        "emoji": { "type": "boolean" }
      },
      "required": ["type", "text"],
      "additionalProperties": false
    },
    "plainTextObject30": {
      "allOf": [
        { "$ref": "#/$defs/plainTextObject" },
        { "type": "object", "properties": { "text": { "maxLength": 30 } } }
      ]
    },
    "plainTextObject75": {
      "allOf": [
        { "$ref": "#/$defs/plainTextObject" },
        { "type": "object", "properties": { "text": { "maxLength": 75 } } }
      ]
    },
    "plainTextObject100": {
      "allOf": [
        { "$ref": "#/$defs/plainTextObject" },
        { "type": "object", "properties": { "text": { "maxLength": 100 } } }
      ]
    },
    "plainTextObject150": {
      "allOf": [
        { "$ref": "#/$defs/plainTextObject" },
        { "type": "object", "properties": { "text": { "maxLength": 150 } } }
      ]
    },
    "plainTextObject200": {
      "allOf": [
        { "$ref": "#/$defs/plainTextObject" },
        { "type": "object", "properties": { "text": { "maxLength": 200 } } }
      ]
    },
    "plainTextObject300": {
      "allOf": [
        { "$ref": "#/$defs/plainTextObject" },
        { "type": "object", "properties": { "text": { "maxLength": 300 } } }
      ]
    },
    "plainTextObject1500": {
      "allOf": [
        { "$ref": "#/$defs/plainTextObject" },
        { "type": "object", "properties": { "text": { "maxLength": 1500 } } }
      ]
    },
    "plainTextObject2000": {
      "allOf": [
        { "$ref": "#/$defs/plainTextObject" },
        { "type": "object", "properties": { "text": { "maxLength": 2000 } } }
      ]
    },
    "mrkdwnTextObject": {
      "type": "object",
      "properties": {
        "type": { "const": "mrkdwn" },
        "text": { "type": "string", "minLength": 1, "maxLength": 3000 },
        "verbatim": { "type": "boolean" }
      },
      "required": ["type", "text"],
      "additionalProperties": false
    },
    "textObject": {
      "oneOf": [
        { "$ref": "#/$defs/plainTextObject" },
        { "$ref": "#/$defs/mrkdwnTextObject" }
      ]
    },
    "rawTextObject": {
      "type": "object",
      "properties": {
        "type": { "const": "raw_text" },
        "text": { "type": "string", "minLength": 1, "maxLength": 10000 }
      },
      "required": ["type", "text"],
      "additionalProperties": false
    },
    "confirmObject": {
      "type": "object",
      "properties": {
        "title": { "$ref": "#/$defs/plainTextObject100" },
        "text": { "$ref": "#/$defs/plainTextObject300" },
        "confirm": { "$ref": "#/$defs/plainTextObject30" },
        "deny": { "$ref": "#/$defs/plainTextObject30" },
        "style": { "enum": ["primary", "danger"] }
      },
      "required": ["title", "text", "confirm", "deny"],
      "additionalProperties": false
    },
    "optionObject": {
      "type": "object",
      "properties": {
        "text": { "$ref": "#/$defs/plainTextObject75" },
        "value": { "type": "string", "minLength": 1, "maxLength": 150 },
        "description": { "$ref": "#/$defs/plainTextObject75" },
        "url": { "$ref": "#/$defs/url" }
      },
      "required": ["text", "value"],
      "additionalProperties": false
    },
    "optionGroupObject": {
      "type": "object",
      "properties": {
        "label": { "$ref": "#/$defs/plainTextObject75" },
        "options": {
          "type": "array",
          "minItems": 1,
          "maxItems": 100,
          "items": { "$ref": "#/$defs/optionObject" }
        }
      },
      "required": ["label", "options"],
      "additionalProperties": false
    },
    "dispatchActionConfigObject": {
      "type": "object",
      "properties": {
        "trigger_actions_on": {
          "type": "array",
          "minItems": 1,
          "items": {
            "enum": ["on_enter_pressed", "on_character_entered"]
          }
        }
      },
      "required": ["trigger_actions_on"],
      "additionalProperties": false
    },
    "conversationFilterObject": {
      "type": "object",
      "properties": {
        "include": {
          "type": "array",
          "minItems": 1,
          "items": {
            "enum": ["im", "mpim", "private", "public"]
          }
        },
        "exclude_bot_users": { "type": "boolean" },
        "exclude_external_shared_channels": { "type": "boolean" }
      },
      "additionalProperties": false,
      "anyOf": [
        { "required": ["include"] },
        { "required": ["exclude_bot_users"] },
        { "required": ["exclude_external_shared_channels"] }
      ]
    },
    "inputParameterObject": {
      "type": "object",
      "properties": {
        "name": { "type": "string", "minLength": 1, "maxLength": 3000 },
        "value": { "type": "string", "minLength": 1, "maxLength": 3000 }
      },
      "required": ["name", "value"],
      "additionalProperties": false
    },
    "triggerObject": {
      "type": "object",
      "properties": {
        "url": { "$ref": "#/$defs/url" },
        "customizable_input_parameters": {
          "type": "array",
          "minItems": 1,
          "maxItems": 100,
          "items": { "$ref": "#/$defs/inputParameterObject" }
        }
      },
      "required": ["url"],
      "additionalProperties": false
    },
    "workflowObject": {
      "type": "object",
      "properties": {
        "trigger": { "$ref": "#/$defs/triggerObject" }
      },
      "required": ["trigger"],
      "additionalProperties": false
    },
    "slackFileObject": {
      "type": "object",
      "properties": {
        "id": { "type": "string", "minLength": 1, "maxLength": 30 },
        "url": { "$ref": "#/$defs/url" }
      },
      "additionalProperties": false,
      "oneOf": [
        { "required": ["id"] },
        { "required": ["url"] }
      ]
    },
    "richTextStyleObject": {
      "type": "object",
      "properties": {
        "bold": { "type": "boolean" },
        "italic": { "type": "boolean" },
        "strike": { "type": "boolean" },
        "code": { "type": "boolean" },
        "highlight": { "type": "boolean" },
        "client_highlight": { "type": "boolean" },
        "unlink": { "type": "boolean" }
      },
      "additionalProperties": false
    },
    "richTextElementText": {
      "type": "object",
      "properties": {
        "type": { "const": "text" },
        "text": { "type": "string", "minLength": 1, "maxLength": 3000 },
        "style": { "$ref": "#/$defs/richTextStyleObject" }
      },
      "required": ["type", "text"],
      "additionalProperties": false
    },
    "richTextElementEmoji": {
      "type": "object",
      "properties": {
        "type": { "const": "emoji" },
        "name": { "type": "string", "minLength": 1 },
        "unicode": { "type": "string", "minLength": 1 }
      },
      "required": ["type", "name"],
      "additionalProperties": false
    },
    "richTextElementLink": {
      "type": "object",
      "properties": {
        "type": { "const": "link" },
        "url": { "$ref": "#/$defs/url" },
        "text": { "type": "string", "minLength": 1 },
        "unsafe": { "type": "boolean" },
        "style": { "$ref": "#/$defs/richTextStyleObject" }
      },
      "required": ["type", "url"],
      "additionalProperties": false
    },
    "richTextElementUser": {
      "type": "object",
      "properties": {
        "type": { "const": "user" },
        "user_id": { "type": "string", "minLength": 1 },
        "style": { "$ref": "#/$defs/richTextStyleObject" }
      },
      "required": ["type", "user_id"],
      "additionalProperties": false
    },
    "richTextElementUsergroup": {
      "type": "object",
      "properties": {
        "type": { "const": "usergroup" },
        "usergroup_id": { "type": "string", "minLength": 1 },
        "style": { "$ref": "#/$defs/richTextStyleObject" }
      },
      "required": ["type", "usergroup_id"],
      "additionalProperties": false
    },
    "richTextElementChannel": {
      "type": "object",
      "properties": {
        "type": { "const": "channel" },
        "channel_id": { "type": "string", "minLength": 1 },
        "style": { "$ref": "#/$defs/richTextStyleObject" }
      },
      "required": ["type", "channel_id"],
      "additionalProperties": false
    },
    "richTextElementDate": {
      "type": "object",
      "properties": {
        "type": { "const": "date" },
        "timestamp": { "type": "integer" },
        "format": { "type": "string", "minLength": 1 },
        "url": { "$ref": "#/$defs/url" },
        "fallback": { "type": "string", "minLength": 1 }
      },
      "required": ["type", "timestamp", "format"],
      "additionalProperties": false
    },
    "richTextElementBroadcast": {
      "type": "object",
      "properties": {
        "type": { "const": "broadcast" },
        "range": { "enum": ["here", "channel", "everyone"] }
      },
      "required": ["type", "range"],
      "additionalProperties": false
    },
    "richTextElementColor": {
      "type": "object",
      "properties": {
        "type": { "const": "color" },
        "value": { "type": "string", "pattern": "^#?([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$" }
      },
      "required": ["type", "value"],
      "additionalProperties": false
    },
    "richTextElement": {
      "oneOf": [
        { "$ref": "#/$defs/richTextElementText" },
        { "$ref": "#/$defs/richTextElementEmoji" },
        { "$ref": "#/$defs/richTextElementLink" },
        { "$ref": "#/$defs/richTextElementUser" },
        { "$ref": "#/$defs/richTextElementUsergroup" },
        { "$ref": "#/$defs/richTextElementChannel" },
        { "$ref": "#/$defs/richTextElementDate" },
        { "$ref": "#/$defs/richTextElementBroadcast" },
        { "$ref": "#/$defs/richTextElementColor" }
      ]
    },
    "richTextSectionObject": {
      "type": "object",
      "properties": {
        "type": { "const": "rich_text_section" },
        "elements": {
          "type": "array",
          "minItems": 1,
          "items": { "$ref": "#/$defs/richTextElement" }
        }
      },
      "required": ["type", "elements"],
      "additionalProperties": false
    },
    "richTextListObject": {
      "type": "object",
      "properties": {
        "type": { "const": "rich_text_list" },
        "style": { "enum": ["bullet", "ordered"] },
        "elements": {
          "type": "array",
          "minItems": 1,
          "items": { "$ref": "#/$defs/richTextSectionObject" }
        },
        "indent": { "type": "integer", "minimum": 0, "maximum": 8 },
        "offset": { "type": "integer", "minimum": 0 },
        "border": { "type": "integer", "minimum": 0, "maximum": 1 }
      },
      "required": ["type", "style", "elements"],
      "additionalProperties": false
    },
    "richTextPreformattedObject": {
      "type": "object",
      "properties": {
        "type": { "const": "rich_text_preformatted" },
        "elements": {
          "type": "array",
          "minItems": 1,
          "items": { "$ref": "#/$defs/richTextElement" }
        },
        "border": { "type": "integer", "minimum": 0, "maximum": 1 }
      },
      "required": ["type", "elements"],
      "additionalProperties": false
    },
    "richTextQuoteObject": {
      "type": "object",
      "properties": {
        "type": { "const": "rich_text_quote" },
        "elements": {
          "type": "array",
          "minItems": 1,
          "items": { "$ref": "#/$defs/richTextElement" }
        },
        "border": { "type": "integer", "minimum": 0, "maximum": 1 }
      },
      "required": ["type", "elements"],
      "additionalProperties": false
    },
    "richTextBlock": {
      "type": "object",
      "properties": {
        "type": { "const": "rich_text" },
        "elements": {
          "type": "array",
          "minItems": 1,
          "items": {
            "oneOf": [
              { "$ref": "#/$defs/richTextSectionObject" },
              { "$ref": "#/$defs/richTextListObject" },
              { "$ref": "#/$defs/richTextPreformattedObject" },
              { "$ref": "#/$defs/richTextQuoteObject" }
            ]
          }
        },
        "block_id": { "$ref": "#/$defs/blockId" }
      },
      "required": ["type", "elements"],
      "additionalProperties": false
    },
    "columnSettingsObject": {
      "type": "object",
      "properties": {
        "align": { "enum": ["left", "center", "right"] },
        "is_wrapped": { "type": "boolean" }
      },
      "additionalProperties": false
    },
    "buttonElement": {
      "type": "object",
      "properties": {
        "type": { "const": "button" },
        "text": { "$ref": "#/$defs/plainTextObject75" },
        "action_id": { "$ref": "#/$defs/actionId" },
        "url": { "$ref": "#/$defs/url" },
        "value": { "type": "string", "minLength": 1, "maxLength": 2000 },
        "style": { "enum": ["primary", "danger"] },
        "confirm": { "$ref": "#/$defs/confirmObject" },
        "accessibility_label": { "type": "string", "minLength": 1, "maxLength": 75 }
      },
      "required": ["type", "text", "action_id"],
      "additionalProperties": false
    },
    "checkboxesElement": {
      "type": "object",
      "properties": {
        "type": { "const": "checkboxes" },
        "action_id": { "$ref": "#/$defs/actionId" },
        "options": {
          "type": "array",
          "minItems": 1,
          "maxItems": 10,
          "items": { "$ref": "#/$defs/optionObject" }
        },
        "initial_options": {
          "type": "array",
          "minItems": 1,
          "maxItems": 10,
          "items": { "$ref": "#/$defs/optionObject" }
        },
        "confirm": { "$ref": "#/$defs/confirmObject" },
        "focus_on_load": { "type": "boolean" }
      },
      "required": ["type", "action_id", "options"],
      "additionalProperties": false
    },
    "datePickerElement": {
      "type": "object",
      "properties": {
        "type": { "const": "datepicker" },
        "action_id": { "$ref": "#/$defs/actionId" },
        "initial_date": { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}$" },
        "confirm": { "$ref": "#/$defs/confirmObject" },
        "focus_on_load": { "type": "boolean" },
        "placeholder": { "$ref": "#/$defs/plainTextObject150" }
      },
      "required": ["type", "action_id"],
      "additionalProperties": false
    },
    "datetimePickerElement": {
      "type": "object",
      "properties": {
        "type": { "const": "datetimepicker" },
        "action_id": { "$ref": "#/$defs/actionId" },
        "initial_date_time": { "type": "integer" },
        "confirm": { "$ref": "#/$defs/confirmObject" },
        "focus_on_load": { "type": "boolean" },
        "placeholder": { "$ref": "#/$defs/plainTextObject150" }
      },
      "required": ["type", "action_id"],
      "additionalProperties": false
    },
    "emailInputElement": {
      "type": "object",
      "properties": {
        "type": { "const": "email_text_input" },
        "action_id": { "$ref": "#/$defs/actionId" },
        "initial_value": { "type": "string", "minLength": 1, "maxLength": 74 },
        "dispatch_action_config": { "$ref": "#/$defs/dispatchActionConfigObject" },
        "focus_on_load": { "type": "boolean" },
        "placeholder": { "$ref": "#/$defs/plainTextObject150" }
      },
      "required": ["type", "action_id"],
      "additionalProperties": false
    },
    "fileInputElement": {
      "type": "object",
      "properties": {
        "type": { "const": "file_input" },
        "action_id": { "$ref": "#/$defs/actionId" },
        "filetypes": {
          "type": "array",
          "items": {
            "enum": [
              "auto", "text", "ai", "apk", "applescript", "binary", "bmp", "boxnote", "c",
              "csharp", "cpp", "css", "csv", "clojure", "coffeescript", "cfm", "d", "dart",
              "diff", "doc", "docx", "dockerfile", "dotx", "eml", "eps", "epub", "erlang",
              "fla", "flv", "fsharp", "fortran", "gdoc", "gdraw", "gif", "go", "gpres",
              "groovy", "gsheet", "gzip", "html", "handlebars", "haskell", "haxe", "indd",
              "java", "javascript", "jpg", "json", "keynote", "kotlin", "latex", "lisp",
              "lua", "m4a", "markdown", "matlab", "mhtml", "mkv", "mov", "mp3", "mp4",
              "mpg", "mumps", "numbers", "nzb", "objc", "ocaml", "odg", "odi", "odp", "ods",
              "odt", "ogg", "ogv", "pages", "pascal", "pdf", "perl", "php", "pig", "png",
              "post", "powershell", "ppt", "pptx", "psd", "puppet", "python", "qtz", "r",
              "rtf", "ruby", "rust", "sql", "sass", "scala", "scheme", "sketch", "shell",
              "smalltalk", "svg", "swf", "swift", "tar", "tiff", "tsv", "vb", "vbscript",
              "vcard", "velocity", "verilog", "wav", "webm", "wmv", "xls", "xlsx", "xlsb",
              "xlsm", "xltx", "xml", "yaml", "zip"
            ]
          }
        },
        "max_files": { "type": "integer", "minimum": 1, "maximum": 10 }
      },
      "required": ["type", "action_id"],
      "additionalProperties": false
    },
    "imageElement": {
      "type": "object",
      "properties": {
        "type": { "const": "image" },
        "alt_text": { "type": "string", "minLength": 1, "maxLength": 255 },
        "image_url": { "$ref": "#/$defs/url" },
        "slack_file": { "$ref": "#/$defs/slackFileObject" }
      },
      "required": ["type", "alt_text"],
      "additionalProperties": false,
      "oneOf": [
        { "required": ["image_url"] },
        { "required": ["slack_file"] }
      ]
    },
    "multiStaticSelectElement": {
      "type": "object",
      "properties": {
        "type": { "const": "multi_static_select" },
        "action_id": { "$ref": "#/$defs/actionId" },
        "options": {
          "type": "array",
          "minItems": 1,
          "maxItems": 100,
          "items": { "$ref": "#/$defs/optionObject" }
        },
        "option_groups": {
          "type": "array",
          "minItems": 1,
          "maxItems": 100,
          "items": { "$ref": "#/$defs/optionGroupObject" }
        },
        "initial_options": {
          "type": "array",
          "minItems": 1,
          "maxItems": 100,
          "items": {
            "oneOf": [
              { "$ref": "#/$defs/optionObject" },
              { "$ref": "#/$defs/optionGroupObject" }
            ]
          }
        },
        "confirm": { "$ref": "#/$defs/confirmObject" },
        "max_selected_items": { "type": "integer", "minimum": 1 },
        "focus_on_load": { "type": "boolean" },
        "placeholder": { "$ref": "#/$defs/plainTextObject150" }
      },
      "required": ["type", "action_id"],
      "additionalProperties": false,
      "oneOf": [
        { "required": ["options"] },
        { "required": ["option_groups"] }
      ]
    },
    "multiExternalSelectElement": {
      "type": "object",
      "properties": {
        "type": { "const": "multi_external_select" },
        "action_id": { "$ref": "#/$defs/actionId" },
        "min_query_length": { "type": "integer", "minimum": 1 },
        "initial_options": {
          "type": "array",
          "minItems": 1,
          "maxItems": 100,
          "items": {
            "oneOf": [
              { "$ref": "#/$defs/optionObject" },
              { "$ref": "#/$defs/optionGroupObject" }
            ]
          }
        },
        "confirm": { "$ref": "#/$defs/confirmObject" },
        "max_selected_items": { "type": "integer", "minimum": 1 },
        "focus_on_load": { "type": "boolean" },
        "placeholder": { "$ref": "#/$defs/plainTextObject150" }
      },
      "required": ["type", "action_id"],
      "additionalProperties": false
    },
    "multiUsersSelectElement": {
      "type": "object",
      "properties": {
        "type": { "const": "multi_users_select" },
        "action_id": { "$ref": "#/$defs/actionId" },
        "initial_users": {
          "type": "array",
          "minItems": 1,
          "items": { "type": "string", "minLength": 1 }
        },
        "confirm": { "$ref": "#/$defs/confirmObject" },
        "max_selected_items": { "type": "integer", "minimum": 1 },
        "focus_on_load": { "type": "boolean" },
        "placeholder": { "$ref": "#/$defs/plainTextObject150" }
      },
      "required": ["type", "action_id"],
      "additionalProperties": false
    },
    "multiConversationsSelectElement": {
      "type": "object",
      "properties": {
        "type": { "const": "multi_conversations_select" },
        "action_id": { "$ref": "#/$defs/actionId" },
        "initial_conversations": {
          "type": "array",
          "minItems": 1,
          "items": { "type": "string", "minLength": 1 }
        },
        "default_to_current_conversation": { "type": "boolean" },
        "confirm": { "$ref": "#/$defs/confirmObject" },
        "max_selected_items": { "type": "integer", "minimum": 1 },
        "filter": { "$ref": "#/$defs/conversationFilterObject" },
        "focus_on_load": { "type": "boolean" },
        "placeholder": { "$ref": "#/$defs/plainTextObject150" }
      },
      "required": ["type", "action_id"],
      "additionalProperties": false
    },
    "multiChannelsSelectElement": {
      "type": "object",
      "properties": {
        "type": { "const": "multi_channels_select" },
        "action_id": { "$ref": "#/$defs/actionId" },
        "initial_channels": {
          "type": "array",
          "minItems": 1,
          "items": { "type": "string", "minLength": 1 }
        },
        "confirm": { "$ref": "#/$defs/confirmObject" },
        "max_selected_items": { "type": "integer", "minimum": 1 },
        "focus_on_load": { "type": "boolean" },
        "placeholder": { "$ref": "#/$defs/plainTextObject150" }
      },
      "required": ["type", "action_id"],
      "additionalProperties": false
    },
    "numberInputElement": {
      "type": "object",
      "properties": {
        "type": { "const": "number_input" },
        "is_decimal_allowed": { "type": "boolean" },
        "action_id": { "$ref": "#/$defs/actionId" },
        "initial_value": { "type": ["number", "string"] },
        "min_value": { "type": ["number", "string"] },
        "max_value": { "type": ["number", "string"] },
        "dispatch_action_config": { "$ref": "#/$defs/dispatchActionConfigObject" },
        "focus_on_load": { "type": "boolean" },
        "placeholder": { "$ref": "#/$defs/plainTextObject150" }
      },
      "required": ["type", "action_id", "is_decimal_allowed"],
      "additionalProperties": false
